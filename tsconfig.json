{
  "extends": "astro/tsconfigs/strictest",
  "include": [
    ".astro/types.d.ts",
    "**/*"
  ],
  "exclude": [
    "dist"
  ],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "~/*": [
        "./*"
      ],
      "@*": [
        "./src/*"
      ]
    },
    "noEmit": true,
    "jsx": "preserve",
    "jsxImportSource": "@astrojs/renderer-preact"
  },
}
